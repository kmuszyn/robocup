		<geom:trimesh name="main_cylinder">
			<xyz>0 0 0.04</xyz>
			<!--<size>0.06 0.06</size>-->	<!--r: 6 cm, h: 4 cm -->
			<mass>10</mass>
			<kp>2e0</kp>
			<kd>2e6</kd>	
			<mesh>test.mesh</mesh>
			<visual>
				<scale>1 1 1</scale>
				<mesh>test.mesh</mesh>
				<material>Gazebo/Blue</material>
			</visual>
		</geom:trimesh>
		
		<!--dribbler supporters -->
		<geom:box name="dribbler_sup_L"> <!-- left one -->
			<xyz>-0.035 0.060 0.010</xyz>
			<size>0.01 0.04 0.04</size>
			<mass>0.1</mass>
			<kp>0</kp>
			<kd>1e7</kd>
			<bounce>0</bounce>
			<bounceVel>0</bounceVel>
			<mu1>0</mu1>
			<mu2>0</mu2>
			<visual>
				<scale>0.01 0.04 0.04</scale>
				<mesh>unit_box</mesh>
				<material>Gazebo/Blue</material>
			</visual>
		</geom:box>
		
		<geom:box name="dribbler_sup_R"> <!-- right one -->
			<xyz>0.035 0.060 0.010</xyz>
			<size>0.01 0.04 0.04</size>
			<mass>0.1</mass>
			<kp>0</kp>
			<kd>1e7</kd>
			<bounce>0</bounce>
			<bounceVel>0</bounceVel>
			<mu1>0</mu1>
			<mu2>0</mu2>
			<visual>
				<scale>0.01 0.04 0.04</scale>
				<mesh>unit_box</mesh>
				<material>Gazebo/Blue</material>
			</visual>
		</geom:box>	
		
		<!--
		<geom:box name="dribbler_stopping_bar">
			<xyz>0 0.075 -0.01</xyz>
			<size>0.060 0.006 0.010</size>
			<mass>0.01</mass>			
			<kp>1</kp>
			<kd>2e6</kd>
