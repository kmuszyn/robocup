		<geom:cylinder name="dribbler_stopping_bar">	
			<!--<xyz>0 0.075 -0.005</xyz>	-->
			<xyz>0 0.072 -0.005</xyz>	
			<rpy>0 90 0</rpy>
			<size>0.003 0.060</size>
			<mass>0.01</mass>
			<mu1>0.001</mu1>
			<kp>2e0</kp>
			<kd>2e6</kd>
			<bounce>0</bounce>
			<visual>				
				<scale>0.006 0.006 0.060</scale>
				<mesh>unit_cylinder</mesh>
				<material>Gazebo/Chrome</material>
			</visual>
		</geom:cylinder>
		
	</body:trimesh>
	
	<!-- omni - wheels -->
	<!--
	
	front left wheel
	   |
	   x x - front right wheel
		O
		x
		|
		rear wheel
	-->
	
	
	
	<body:cylinder name="rear_wheel">
		<xyz>0 -0.075 0</xyz>
		<rpy>90 0 0</rpy>		
		<geom:cylinder name="rear_wheel_geom">		
			<size>0.02 0.030</size>
			<mass>2</mass>
			
			<visual>				
				<scale>0.04 0.04 0.030</scale>
				<mesh>unit_cylinder</mesh>
				<material>Gazebo/Black</material>
			</visual>
			
			<kp>2e6</kp>
			<kd>1000</kd>	
			
			<mu1>100</mu1>
			<mu2>0.01</mu2>
			<fDir1>1 0 0</fDir1>
			
			<slip1>0.001</slip1><slip2>1</slip2>

		</geom:cylinder>
	</body:cylinder> 
	
	<body:cylinder name="front_right_wheel">
		<xyz>0.0649519 0.0375  0</xyz>
		<rpy>90 0 -60</rpy>		
		<geom:cylinder name="front_right_wheel_geom">		
			<size>0.02 0.030</size>
			<mass>2</mass>
			
			<kp>2e6</kp>
			<kd>1000</kd>	

			
			<visual>				
				<scale>0.04 0.04 0.030</scale>
				<mesh>unit_cylinder</mesh>
				<material>Gazebo/Black</material>
			</visual>


			<mu1>100</mu1>
			<mu2>0.01</mu2>				
			<fDir1>1 0 0</fDir1>
			
			<slip1>0.001</slip1><slip2>1</slip2>

		</geom:cylinder>
	</body:cylinder> 
	
	<body:cylinder name="front_left_wheel">
		<xyz>-0.0649519 0.0375  0</xyz>
		<rpy>90 0 60</rpy>		
		<geom:cylinder name="front_left_wheel_geom">		
			<size>0.02 0.030</size>
			<mass>2</mass>
			
			<kp>2e6</kp>
			<kd>1000</kd>	
			
			<visual>				
				<scale>0.04 0.04 0.030</scale>
